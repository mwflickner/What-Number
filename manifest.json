{
  "manifest_version": 2,

  "name": "What Number?",
  "description": "Make Chrome Biz Again",
  "version": "1.0",

  "browser_action": {
    "default_icon": "images/biz_logo.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
        "matches": [
        "http://*/*",
        "https://*/*"
        ],
        
        "js": [
            "js/jquery-2.2.2.min.js",
            "js/changeToBiz.js"
        ],
        "run_at": "document_start"
    }
  ]
}
